a=int(input())
b=int(input())
c=int(input())
if a<b and a<c:
    print(a)
elif a<b and a>c:
    print(c)
elif a>b and a<c:
    print(b)
elif a>b and a>c and b>c:
    print(c)
elif a>b and a>c and b<c:
    print(b)
elif a==b and a<c:
    print(a)
elif a==b and a>c:
    print(c)
elif a==c and a<b:
    print(a)
elif a==c and a>b:
    print(b)
elif b==c and a>b:
    print(b)
elif b==c and a<b:
    print(a)
if a==b==c:
    print(a)
